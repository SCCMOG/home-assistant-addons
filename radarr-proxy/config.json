{
    "name": "Radarr Proxy",
    "version": "1.0.0",
    "slug": "radarr_proxy",
    "description": "Proxy for externally running Radarr",
    "url": "https://github.com/SCCMOG/home-assistant-addons/tree/master/radarr-proxy",
    "arch": [
      "aarch64",
      "amd64",
      "armhf",
      "armv7",
      "i386"
    ],
    "boot": "auto",
    "init": true,
    "true": true,
    "ingress": true,
    "timeout": 30,
    "panel_icon": "mdi:radarr",
    "map": [
      "config:ro"
    ],
    "ports": {
      "7878/tcp": null
    },
    "ports_description": {
      "7878/tcp": "Frontend tcp-listen port"
    },
    "options": {
      "server": null,
      "auth_token": ""
    },
    "schema": {
      "server": "url",
      "auth_token": "str?"
    }
}
